{
    "name": "algodebug",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve --port 8081",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint",
        "format": "prettier --write --list-different .",
        "format:check": "prettier --check ."
    },
    "dependencies": {
        "axios": "^1.1.3",
        "core-js": "^3.26.0",
        "cors": "^2.8.5",
        "jenesius-vue-modal": "^1.8.2",
        "konva": "^8.3.13",
        "mitt": "^3.0.0",
        "vue": "^3.2.41",
        "vue-3-sanitize": "^0.1.4",
        "vue-konva": "^3.0.1",
        "vue-modaltor": "^1.5.4",
        "vue-universal-modal": "^1.1.4",
        "vuetify": "^3.0.4",
        "vuex": "^4.1.0"
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.19.1",
        "@vue/cli-plugin-babel": "^5.0.8",
        "@vue/cli-plugin-eslint": "^5.0.8",
        "@vue/cli-service": "^5.0.8",
        "@vue/compiler-sfc": "^3.2.41",
        "eslint": "^8.26.0",
        "eslint-plugin-vue": "^9.7.0",
        "prettier": "^2.8.0",
        "sass": "^1.56.2",
        "sass-loader": "^13.2.0"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/vue3-essential",
            "eslint:recommended"
        ],
        "parserOptions": {
            "parser": "@babel/eslint-parser"
        },
        "rules": {
            "vue/multi-word-component-names": [
                "error",
                {
                    "ignores": [
                        "Menu",
                        "Scene",
                        "Codearea"
                    ]
                }
            ]
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead"
    ]
}
